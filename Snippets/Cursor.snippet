<CodeSnippets  
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">  
    <CodeSnippet Format="1.0.0">  
        <Header>  
            <Title>Cursor</Title>  
        </Header>  
        <Snippet>  
            <Code Language="SQL">  
                <![CDATA[
DECLARE DBcursor CURSOR  FOR 
select 
	<column>
from <table>

OPEN DBcursor; FETCH DBcursor INTO @XXXXXXXXX; WHILE (@@FETCH_STATUS = 0)
BEGIN  	
	<do_something>
	FETCH DBcursor INTO @XXXXXXXXX
END

CLOSE DBCURSOR; DEALLOCATE DBCURSOR
				]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
